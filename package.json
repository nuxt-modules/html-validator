{
  "name": "@nuxtjs/html-validator",
  "version": "1.4.0",
  "description": "html-validate integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "html-validator",
    "validation",
    "html"
  ],
  "repository": "nuxt-modules/html-validator",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist",
    "validator.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue",
    "lint:all": "pnpm lint .",
    "release": "pnpm test && bumpp && npm publish",
    "test": "pnpm vitest run"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.1",
    "chalk": "^5.1.2",
    "html-validate": "~7.18.0",
    "pathe": "^1.0.0",
    "prettier": "^2.7.1",
    "std-env": "^3.3.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.4.0",
    "@nuxt/test-utils": "3.6.1",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "bumpp": "9.1.1",
    "c8": "7.14.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "nuxt": "3.6.1",
    "vitest": "0.32.2"
  },
  "resolutions": {
    "@nuxtjs/html-validator": "link:./",
    "consola": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.5"
}
